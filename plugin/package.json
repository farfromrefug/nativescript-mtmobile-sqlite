{
    "name": "@akylas/nativescript-sqlite",
    "version": "3.3.11",
    "description": "SQLite for Nativescript",
    "main": "sqlite",
    "typings": "sqlite.d.ts",
    "sideEffects": false,
    "scripts": {
        "preuninstall": "node preuninstall.js",
        "postinstall": "node postinstall.js"
    },
    "nativescript": {
        "platforms": {
            "android": "6.2.0",
            "ios": "6.2.0"
        },
        "hooks": [
            {
                "type": "before-prepare",
                "script": "scripts/before-prepare.js",
                "inject": true
            }
        ]
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS"
    ],
    "author": {
        "name": "Josep Bergay",
        "email": "josep.bergay@gmail.com"
    },
    "bugs": {
        "url": "https://github.com/TestJG/nativescript-akylas-sqlite/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/TestJG/nativescript-akylas-sqlite",
    "readmeFilename": "README.md",
    "repository": "TestJG/nativescript-akylas-sqlite",
    "dependencies": {
        "@nano-sql/core": "^2.3.7",
        "@nativescript-community/typeorm": "*",
        "@nativescript/hook": "~2.0.0"
    }
}
